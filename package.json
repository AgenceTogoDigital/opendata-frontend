{
  "name": "udata-gouvfr",
  "version": "1.0.0",
  "description": "Open Data Portal - French customization",
  "author": "Etalab",
  "license": "MIT",
  "scripts": {
    "format": "stylelint 'theme/less/**/*.less' --fix",
    "check-rules": "stylelint 'theme/less/**/*.less' --config .stylelintrc_browser",
    "watch-style": "parcel theme/less/index.less -d udata_gouvfr/theme/static/",
    "build-stylemark": "parcel build theme/less/index.less -d udata_gouvfr/theme/static/stylemark --no-minify && stylemark -c .stylemark.yml -i udata_gouvfr/theme/static/stylemark/ -o udata_gouvfr/theme/stylemark/",
    "build-style": "parcel build theme/less/index.less -d udata_gouvfr/theme/static/",
    "dev": "concurrently --restart-tries 5 'npm:watch-style'",
    "build": "npm run-script build-style && npm run-script build-stylemark",
    "start": "npm run-script dev"
  },
  "dependencies": {
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.0.0",
    "commitizen": "^4.1.2",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "cz-emoji": "^1.2.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "less": "^3.12.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.0",
    "postcss-normalize": "^9.0.0",
    "prettier": "2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.1.0",
    "stylemark": "^3.1.7"
  }
}
