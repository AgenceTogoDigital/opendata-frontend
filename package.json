{
  "name": "udata-gouvfr",
  "version": "1.0.0",
  "description": "Open Data Portal - French customization",
  "author": "Etalab",
  "scripts": {
    "build": "npm run clean && npm run build-app && npm run build-stylemark",
    "build-app": "parcel build theme/less/index.less theme/js/index.js --dist-dir udata_gouvfr/theme/static/",
    "build-stylemark": "npm run clean && parcel build theme/less/index.less theme/js/index.js --dist-dir udata_gouvfr/theme/static/stylemark_temp --no-minify && stylemark -c .stylemark.yml -i udata_gouvfr/theme/static/stylemark_temp/ -o udata_gouvfr/theme/static/stylemark/",
    "report:i18n": "npx vue-i18n-extract report -v 'theme/**/*.vue' -l 'theme/js/locales/**/*'",
    "clean": "rimraf .parcel-cache",
    "dev": "npm run clean && npm run watch-app",
    "start": "npm run dev",
    "watch-app": "parcel theme/js/index.js theme/less/index.less --dist-dir udata_gouvfr/theme/static/"
  },
  "dependencies": {
    "@parcel/transformer-less": "^2.0.0-alpha.3",
    "axios": "^0.21.0",
    "dayjs": "^1.9.6",
    "less": "^3.13.1",
    "parcel-reporter-copystatic": "file:parcel-reporter-copystatic",
    "parcel-transformer-more": "file:parcel-transformer-more",
    "popmotion": "^8.7",
    "prettier": "^2.2.1",
    "vue": "^3.0.0-beta.1",
    "vue-final-modal": "^3.0.0-beta.1",
    "vue-i18n": "^9.0.0-rc.2",
    "vue3-clipboard": "^1.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.0.0",
    "cssnano": "^4.1.10",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^8.2.3",
    "postcss-normalize": "^9.0.0",
    "rimraf": "^3.0.2",
    "stylemark": "^3.1.7",
    "vue-i18n-extract": "^1.1.10"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "theme/img",
        "staticOutDir": "udata_gouvfr/theme/static/img/"
      }
    ]
  },
  "alias": {
    "svg": "./udata_gouvfr/theme/templates/svg"
  },
  "license": "MIT"
}
